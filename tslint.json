{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "class-name": true,
        "curly": true,
        "indent": [
            true,
            "spaces"
        ],
        "jsdoc-format": true,
        "max-classes-per-file": [
            true,
            1
        ],
        "new-parens": true,
        "no-empty": true,
        "no-for-in-array": true,
        "no-switch-case-fall-through": true,
        "no-trailing-whitespace": true,
        "no-unused-variable": true,
        "triple-equals": true,
        "use-isnan": true,
        "cyclomatic-complexity": [
            true,
            25
        ],
        "array-type": false,
        "eofline": true,
        "no-eval": false,
        "one-variable-per-declaration": [true],
        "ordered-imports": [true],
        "quotemark": [
            true,
            "double",
            "avoid-escape"
        ],
        "semicolon": [true],
        "return-undefined": true,
        "space-before-function-paren": [
            true,
            "never"
        ],
        "variable-name": [
            true,
            "allow-leading-underscore"
        ],
        "interface-name": [
            false
        ],
        "trailing-comma": [
            false
        ],
        "no-console": [
            false
        ],
        "object-literal-sort-keys": false,
        "no-bitwise": false,
        "radix": false,
        "max-line-length": [
            true,
            150
        ],
        "only-arrow-functions": [
            false
        ],
        "member-ordering": [
            false
        ],
        "adjacent-overload-signatures": false,
        "arrow-parens": false
    },
    "rulesDirectory": []
}